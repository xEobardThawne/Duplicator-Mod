	"authors": [
		"xEobard_Thawne"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "Apache-2.0 license",
	"icon": "assets/duplicatormod/icon.png",
